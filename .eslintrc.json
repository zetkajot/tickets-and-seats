{
  "env": {
  "es2021": true,
  "node": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "./test.tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules":  {
    "@typescript-eslint/no-use-before-define": "off"
  }
}