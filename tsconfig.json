{
  "compilerOptions": {
    "target": "es2017",
    "module": "CommonJS",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
  },
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.spec.ts"]
}